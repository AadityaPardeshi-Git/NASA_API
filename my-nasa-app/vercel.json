{
  "projects": [
    {
      "name": "nasa-api-frontend",
      "directory": "frontend",
      "framework": "create-react-app"
    },
    {
      "name": "nasa-api-backend",
      "directory": "Backend",
      "buildCommand": "npm run build",
      "outputDirectory": "dist"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/api/$1"
    }
  ]
}
